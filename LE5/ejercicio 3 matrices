#include <stdio.h>
#include <stdlib.h>
#include <time.h>

#define N 10 

 int main(){
    int matriz[N][N];
    int valormax = 0;
    int filamax = 0;
    int colmax = 0;
    srand(time(NULL));
    for (int i = 0; i < N; i++) {
        for (int j = 0; j < N; j++) {
            matriz[i][j] = rand() % 100 + 1; 
            printf("%3d ", matriz[i][j]);  
        }printf("\n");
    }
    return 0;
}