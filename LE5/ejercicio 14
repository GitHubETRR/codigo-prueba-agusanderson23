#include <stdio.h>
const int angulointerno= 180;
int main(){
    int angulointerno1;
    int angulointerno2;
    printf("ingrese el valor de dos de los angulos internos del triangulo\n");
    scanf("%d","%d" &angulointerno1,&angulointerno2);
    angulointerno3 = angulointerno-angulointerno1-angulointerno2;
    printf("el angulo restante de su triangulo es %d, angulointerno3);
    return 0;
}
